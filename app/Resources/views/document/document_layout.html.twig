{% extends 'base.html.twig' %}
{# setam linkurile #}
{% set doc_links = {
        'PaymentType':'paymenttype',
        'DocStatus':'docstatus',
        'DocType':'doctype',
    }
%}
{# afisam linkurile #}
{% block submenu %}
    <ul class="nav navbar-nav navbar-left">
        {% for key,link in doc_links %}
            <div class="row top-buffer">
                <li><a href="{{ path(link~'_index') }}">{{ key }}</a></li>
            </div>
        {% endfor %}                       
    </ul>
{% endblock %}
{# blocul body urmeaza a fi rescris in subpagini(de exmeplu in app\Resources\views\product\edit.html.twig) #}
{% block body %}{% endblock %}
